{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2.12",
        "barryvdh/laravel-dompdf": "^3.1",
        "intervention/image": "^3.11",
        "laravel/framework": "^11.0",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^4.6",
        "maatwebsite/excel": "3.1.62",
        "phpoffice/phpspreadsheet": "*",
        "picqer/php-barcode-generator": "^3.2",
        "pusher/pusher-php-server": "^7.2",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-permission": "^6.10"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.3",
        "laravel/pail": "^1.1",
        "laravel/reverb": "^1.5",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "check-php": "@php -r \"if (version_compare(PHP_VERSION, '8.2.12', '<')) { echo 'PHP >= 8.2.12 required!\\n'; exit(1); } else { echo 'PHP version OK: ' . PHP_VERSION . '\\n'; }\"",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fdba74\" \"/opt/lampp/bin/php artisan serve\" \"/opt/lampp/bin/php artisan queue:listen --tries=1\" \"npm run dev\" --names='server,queue,vite'"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.2.12"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "disable-tls": true,
        "secure-http": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
